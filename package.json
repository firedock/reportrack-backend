{
  "name": "reportrack",
  "private": true,
  "version": "0.1.0",
  "description": "A Strapi application",
  "scripts": {
    "dev": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "postbuild": "cp -R build public/admin",
    "test": "jest --forceExit --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "logs:dev": "aws logs tail /aws/elasticbeanstalk/reportrack-backend-dev/var/log/web.stdout.log --region us-west-1 --follow --since 2m",
    "logs:staging": "aws logs tail /aws/elasticbeanstalk/reportrack-backend-staging/var/log/web.stdout.log --region us-west-1 --follow --since 2m",
    "logs:prod": "aws logs tail /aws/elasticbeanstalk/reportrack-backend-prod/var/log/web.stdout.log --region us-west-1 --follow --since 2m",
    "restart:staging": "aws elasticbeanstalk restart-app-server --environment-name reportrack-backend-staging --region us-west-1",
    "restart:prod": "aws elasticbeanstalk restart-app-server --environment-name reportrack-backend-prod --region us-west-1"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "4.20.1",
    "@strapi/plugin-graphql": "^4.25.8",
    "@strapi/plugin-i18n": "^4.25.8",
    "@strapi/plugin-upload": "^4.25.14",
    "@strapi/plugin-users-permissions": "^4.25.8",
    "@strapi/provider-email-nodemailer": "^5.5.1",
    "@strapi/provider-upload-aws-s3": "^4.25.24",
    "@strapi/strapi": "^4.25.8",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "mongodb": "^6.12.0",
    "nodemailer": "^7.0.5",
    "pg": "8.8.0",
    "pg-hstore": "^2.3.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "5.3.4",
    "styled-components": "5.3.3"
  },
  "author": {
    "name": "A Strapi developer"
  },
  "strapi": {
    "uuid": "aced5527-3f46-4c37-9e80-4588543de4f8"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.0.5",
    "supertest": "^7.1.4"
  }
}
