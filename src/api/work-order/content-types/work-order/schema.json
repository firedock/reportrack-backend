{
  "kind": "collectionType",
  "collectionName": "work_orders",
  "info": {
    "singularName": "work-order",
    "pluralName": "work-orders",
    "displayName": "Work Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "account": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::account.account"
    },
    "customer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::customer.customer"
    },
    "property": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::property.property",
      "inversedBy": "work_orders"
    },
    "dueBy": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "New",
        "Open",
        "In Progress",
        "Complete"
      ]
    },
    "private": {
      "type": "boolean"
    },
    "notes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::note.note",
      "mappedBy": "work_order"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images", "files", "videos"]
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
